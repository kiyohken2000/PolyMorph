{
  "expo": {
    "name": "PolyMorph",
    "slug": "PolyMorph",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000,
      "url": "https://u.expo.dev/0fbdab5f-4d6b-4b79-9f6d-e95dd1de34c9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "net.votepurchase.polymorph",
      "buildNumber": "7",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Use the photo library to change your avatar.",
        "CFBundleDevelopmentRegion": "ja_JP",
        "ITSAppUsesNonExemptEncryption": false
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "net.votepurchase.polymorph",
      "versionCode": 8,
      "userInterfaceStyle": "automatic",
      "permissions": [
        "VIBRATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-font",
      "expo-asset",
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "0fbdab5f-4d6b-4b79-9f6d-e95dd1de34c9"
      }
    }
  }
}
